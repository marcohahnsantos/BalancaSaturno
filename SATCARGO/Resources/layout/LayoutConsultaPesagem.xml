<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:local="http://schemas.android.com/apk/res-auto"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<include
		android:id="@+id/toolbar"
		layout="@layout/toolbar"
		android:background="@android:color/background_light"
		 />
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:id="@+id/main_content"
        android:layout_below="@id/toolbar"
        android:minWidth="25px"
        android:minHeight="25px"
        android:background="#ffeeeeee"
        android:contextClickable="false">
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:id="@+id/frameLayout1">
            <ImageView
                android:src="@mipmap/logosat"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/imageView1"
                android:background="@android:color/background_light"/>
        </FrameLayout>
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_marginTop="3dp"
            android:layout_width="match_parent"
            android:layout_height="50.0dp"
            android:id="@+id/frameLayout9">
            <DigitalClock
                android:layout_width="match_parent"
                android:id="@+id/digitalClock2"
                android:gravity="center"
                android:textSize="30dp"
                android:layout_height="47.0dp"
                android:background="#ffddceb7" />
        </FrameLayout>
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:background="@android:color/background_dark"
            android:layout_width="match_parent"
            android:layout_height="2.0dp"
            android:id="@+id/frameLayout14" />
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="40.0dp"
            android:id="@+id/frameLayout19">
            <TextView
                android:text="Data Inicial"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/textView2"
                android:textStyle="normal|bold"
                android:gravity="center" />
        </FrameLayout>
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="60.5dp"
            android:id="@+id/frameLayout10"
            android:layout_marginTop="0dp"
            android:backgroundTint="@android:color/background_light"
            android:background="@android:color/background_light">
            <ImageButton
                android:src="@drawable/calendario"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/BtnDataConsulta"
                android:layout_marginRight="530dp"
                android:hapticFeedbackEnabled="false" />
            <TextView
                android:text="Selecionar Data"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/TextSelConsultaIninical"
                android:layout_marginLeft="97.0dp"
                android:textColor="@android:color/holo_orange_dark"
                android:textStyle="bold|normal"
                android:gravity="center"
                android:textSize="25dp"
                android:background="@android:color/background_light" />
        </FrameLayout>
         <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="40.0dp"
            android:id="@+id/frameLayout19">
            <TextView
                android:text="Data Final"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/textView2"
                android:textStyle="normal|bold"
                android:gravity="center" />
        </FrameLayout>
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="60.5dp"
            android:id="@+id/frameLayout10"
            android:background="@android:color/background_light">
            <ImageButton
                android:src="@drawable/calendario"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/BtnDataConsultaFinal"
                android:layout_marginRight="530dp" />
            <TextView
                android:text="Selecionar Data"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/TextSelConsultaFim"
                android:layout_marginLeft="100dp"
                android:textColor="@android:color/holo_orange_dark"
                android:textStyle="bold|normal"
                android:gravity="center"
                android:textSize="25dp"
                android:background="@android:color/background_light" />
        </FrameLayout>
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:background="@android:color/background_dark"
            android:layout_width="match_parent"
            android:layout_height="2.0dp"
            android:id="@+id/frameLayout14" />
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="63.0dp"
            android:id="@+id/frameLayout20"
            android:layout_marginTop="10dp" >
            <TextView
                android:text="CLIENTE"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/TextPesqCliente"
                android:textStyle="normal|bold"
                android:textSize="25dp"
                android:layout_gravity="left|center"
                android:gravity="center|left"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="450dp" />
            <AutoCompleteTextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/AutoCompleteTextViewCliente"
                android:layout_marginLeft="150dp"
                android:inputType="none|textAutoComplete|textCapCharacters" />
        </FrameLayout>
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="100.5dp"
            android:id="@+id/frameLayout17"
            android:layout_marginTop="10dp">
            <ImageButton
                android:src="@drawable/pesquisar2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/BtnPesquisarPesagem"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="460dp"
                android:background="@android:color/background_light"
                android:layout_marginTop="5dp"
                android:importantForAccessibility="yes" />
            <ImageButton
                android:src="@drawable/voltar"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/BtnVoltar"
                android:layout_marginLeft="470dp"
                android:layout_marginRight="10dp"
                android:background="@android:color/background_light"
                android:layout_marginTop="5dp" />
            <ImageButton
                android:src="@drawable/Printer1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/BtnReimprimirTicket"
                android:layout_marginLeft="160dp"
                android:layout_marginRight="300dp"
                android:background="@android:color/background_light"
                android:layout_marginTop="5dp" />
            <ImageButton
                android:src="@drawable/GerarCsv"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/BtnGerarCsv"
                android:layout_marginLeft="320dp"
                android:layout_marginRight="150dp"
                android:background="@android:color/background_light"
                android:layout_marginTop="5dp" />
        </FrameLayout>
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:background="@android:color/background_dark"
            android:layout_width="match_parent"
            android:layout_height="2.0dp"
            android:id="@+id/frameLayout14" />
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_marginTop="1dp"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:id="@+id/frameLayout2">
            <TextView
                android:text="CÓDIGO        PLACA                                                  PESO TOTAL "
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_width="match_parent"
                android:textSize="20dp"
                android:layout_height="match_parent"
                android:id="@+id/textView1"
                android:background="#ff000000"
                android:backgroundTint="@android:color/black"
                android:textColor="@android:color/background_light"
                android:textStyle="bold|normal"/>
        </FrameLayout>
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="340.0dp"
            android:id="@+id/frameLayout18">
            <ListView
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/ListaPesagens"
                android:textAlignment="textEnd"
                android:headerDividersEnabled="false" />
        </FrameLayout>
    </LinearLayout>
</RelativeLayout>
